<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - RozovaDolina</title>
    <link href="~/css/nikicss.css" rel="stylesheet" />
    <link href="~/css/lightbox.min.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" rel="stylesheet" />
    <script src="~/js/lightbox-plus-jquery.min.js" ></script>
</head>
<body>
    <div class="content">
        <header>
            <ul class="navbar1">

                <li class="nav-item1">
                    <a class="nav-link-b1" asp-area="" asp-controller="Home" asp-action="Index"><img id="logoo" src="~/images/logo.jpg" /></a>
                </li>
                <li class="nav-item2">
                    <a class="nav-link1 " asp-area="" asp-controller="Home" asp-action="Index">За Ансамбъла</a>
                </li>
                <li class="nav-item2">
                    <a class="nav-link1" asp-area="" asp-controller="News" asp-action="">Новини</a>
                </li>
                <li class="nav-item2">
                    <a class="nav-link1" asp-area="" asp-controller="Repertoires" asp-action="">Репертoар</a>
                </li>
                <li class="nav-item2">
                    <a class="nav-link1" asp-area="" asp-controller="Trophies" asp-action="">Награди</a>
                </li>
                <li class="nav-item2">
                    <a class="nav-link1" asp-area="" asp-controller="Messages" asp-action="">Съобщения</a>
                </li>
                <li class="nav-item2">
                    <a class="nav-link1" asp-area="" asp-controller="Home" asp-action="Galery">Галерия</a>
                </li>
                <li class="nav-item2">
                    <a class="nav-link1" asp-area="" asp-controller="Home" asp-action="Contacts">Контанти</a>
                <li class="nav-item2">
                    <a class="nav-link1" asp-area="" asp-controller="Home" asp-action="Sponsors">Спонсори</a>
                </li>
                <partial name="_LoginPartial" />
            </ul>

        </header>
        <main>
            <div class="container">
                <main class="pb-3">
                    @RenderBody()
                </main>
            </div>
        </main>
      



        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        @await RenderSectionAsync("Scripts", required: false)
    </div>
</body>
</html>
